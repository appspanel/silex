{
    "name": "silex/silex",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "https://silex.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "pimple/pimple": "^3.0",
        "symfony/event-dispatcher": "5.0.*",
        "symfony/http-foundation": "5.0.*",
        "symfony/http-kernel": "5.0.*",
        "symfony/routing": "5.0.*"
    },
    "require-dev": {
        "doctrine/dbal": "^2.2",
        "friendsofphp/php-cs-fixer": "^3.4",
        "monolog/monolog": "^1.4.1",
        "swiftmailer/swiftmailer": "^5",
        "symfony/asset": "5.0.*",
        "symfony/browser-kit": "^5.4",
        "symfony/config": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/doctrine-bridge": "5.0.*",
        "symfony/dom-crawler": "5.0.*",
        "symfony/error-handler": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/finder": "5.0.*",
        "symfony/form": "5.0.*",
        "symfony/intl": "5.0.*",
        "symfony/monolog-bridge": "5.0.*",
        "symfony/options-resolver": "5.0.*",
        "symfony/phpunit-bridge": "5.0.*",
        "symfony/process": "5.0.*",
        "symfony/security-bundle": "5.0.*",
        "symfony/serializer": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/twig-bridge": "5.0.*",
        "symfony/validator": "5.0.*",
        "symfony/var-dumper": "5.0.*",
        "symfony/web-link": "5.0.*",
        "twig/twig": "^2.0"
    },
    "replace": {
        "silex/api": "self.version",
        "silex/providers": "self.version"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
